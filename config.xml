 <?xml version="1.0" encoding="UTF-8"?>
<widget xmlns="http://www.w3.org/ns/widgets"
    xmlns:gap="http://phonegap.com/ns/1.0"
    id="com.aupa.sample"
    version="1.0.0">

    <name>sample mobile App with GPS</name>

    <description>
sample web App with java script and html
    </description>

    <author href="http://stepankarslyan.github.com"
email="stepan.karslyan@gmail.com">
Levon
    </author>

    <gap:platforms>
            <gap:platform name="android" minVersion="2.1" />
            <gap:platform name="symbian" />
    </gap:platforms>
    
    <feature name="Camera">
    <param name="android-package" value="org.apache.cordova.camera.CameraLauncher" />
    </feature>
     <feature name="Geolocation">
    <param name="android-package" value="org.apache.cordova.geolocation.GeoBroker" />
    </feature>

</widget>
